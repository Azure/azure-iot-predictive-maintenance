<div class="layout-flex-cell layout-flex-cell-half">
    <div class="layout">
        <div class="layout-row">
            <div class="layout-cell tile">
                <div class="tile-header">
                    <h3>
                        @Resources.TileHeadingSensorHistory
                    </h3>
                </div>
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6 chart-container">
                            <h4>@Resources.TileHeadingSensor9</h4>
                            <div class="chart chart-linechart" data-bind="telemetryLineChart: sensor1Data">
                            </div>
                        </div>

                        <div class="col-md-6 chart-container">
                            <h4>@Resources.TileHeadingSensor11</h4>
                            <div class="chart chart-linechart" data-bind="telemetryLineChart: sensor2Data">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 chart-container">
                            <h4>@Resources.TileHeadingSensor14</h4>
                            <div class="chart chart-linechart" data-bind="telemetryLineChart: sensor3Data">
                            </div>
                        </div>
                        <div class="col-md-6 chart-container">
                            <h4>@Resources.TileHeadingSensor15</h4>
                            <div class="chart chart-linechart" data-bind="telemetryLineChart: sensor4Data">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="layout-row">
            <div class="layout">
                <div class="layout-row">
                    <div class="layout-cell layout-cell-half tile">
                        <div class="tile-header">
                            <h3>
                                @Resources.TileHeadingRul
                                <sub>@Resources.TileHeadingSubheadingInCycles</sub>
                            </h3>
                        </div>
                        <div class="tile-body">
                            <div class="tile-metric">
                                <div class="tile-metric-value" data-bind="text: engine1Rul(), css: { 'tile-metric-value-warning': engine1RulWarning() }"></div>
                                <div class="tile-metric-unit">@Resources.ChartLabelEngine1</div>
                            </div>
                            <div class="tile-metric">
                                <div class="tile-metric-value" data-bind="text: engine2Rul(), css: { 'tile-metric-value-warning': engine2RulWarning() }"></div>
                                <div class="tile-metric-unit">@Resources.ChartLabelEngine2</div>
                            </div>
                        </div>
                    </div>

                    <div class="layout-cell layout-cell-half tile">
                        <div class="tile-header">
                            <h3>
                                @Resources.TileHeadingCycles
                                <sub>#</sub>
                            </h3>
                        </div>
                        <div class="tile-body">
                            <div class="tile-metric">
                                <div class="tile-metric-value" data-bind="text: engine1Cycles()"></div>
                                <div class="tile-metric-unit">@Resources.ChartLabelEngine1</div>
                            </div>
                            <div class="tile-metric">
                                <div class="tile-metric-value" data-bind="text: engine2Cycles()"></div>
                                <div class="tile-metric-unit">@Resources.ChartLabelEngine2</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="layout-row">
            <div class="layout-cell tile">
                <div class="tile-header">
                    <h3>
                        @Resources.TileHeadingRulHistory
                        <sub>@Resources.TileHeadingSubheadingInCycles</sub>
                    </h3>
                </div>
                <div class="chart-container">
                    <div class="chart chart-linechart" data-bind="predictionLineChart: rulData">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
</div>
