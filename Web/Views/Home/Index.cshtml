@using Microsoft.Azure.Devices.Applications.PredictiveMaintenance.Web.App_GlobalResources
@{
    ViewBag.Title = Resources.PageTitleDashboard;
}

<div class="layout-flex layout-space tiles">
<div class="layout-flex-row">
<div class="layout-flex-cell layout-flex-cell-half">
    <div class="layout">
        <div class="layout-row">
            <div class="layout-cell tile">
                <div class="tile-header">
                    <h3>
                        @Resources.TileHeadingAircraftMap
                    </h3>
                </div>
                <!--ko if: simulationState() == 'Stopping'-->
                <div class="indicator indicator-bar indicator-bar-inactive">
                    @Resources.IndicatorLabelStopping
                </div>
                <!--/ko-->
                <!--ko if: simulationState() == 'Starting'-->
                <div class="indicator indicator-bar indicator-bar-inactive">
                    @Resources.IndicatorLabelStarting
                </div>
                <!--/ko-->
                <!--ko if: simulationState() == 'Stopped'-->
                <button class="btn btn-primary tile-button" data-bind="click: startSimulation, disable: sendingCommand">
                    @Resources.ButtonLabelStartSimulation
                </button>
                <div class="indicator indicator-bar indicator-bar-inactive">
                    @Resources.IndicatorLabelStopped
                </div>
                <!--/ko-->
                <!--ko if: simulationState() == 'Started'-->
                <button class="btn btn-primary tile-button" data-bind="click: stopSimulation, disable: sendingCommand">
                    @Resources.ButtonLabelStopSimulation
                </button>
                <div class="indicator indicator-bar indicator-bar-progress">
                    @Resources.IndicatorLabelStarted
                </div>
                <!--/ko-->
